{% extends 'banner.html.twig' %}
{% block content %}
    <div id="lg-form-container">
        <h2>{{ page_heading_2 }}</h2>
        <ul>
            {% for message in messages %}
                <h3>Message</h3>
                {% for detailName, detail in message %}
                    <li>{{ detailName }}: {{ detail }}</li>
                {% endfor %}
            {% endfor %}
        </ul>
        <p class="curr_page"><a href="{{ landing_page }}">Another go</a></p>
    </div>
{% endblock %}